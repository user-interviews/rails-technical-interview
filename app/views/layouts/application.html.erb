<!DOCTYPE html>
<html>
<head>
  <% if content_for?(:title) %>
    <title><%= yield(:title) %> | User Interviews</title>
  <% else %>
    <title>User Interviews</title>
  <% end %>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'shared/header_navbar' %>
<%= react_component 'FlashMessages', messages: flash.to_h unless flash.empty? %>
<%= yield %>

</body>
</html>
